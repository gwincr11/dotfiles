[init]
  templatedir = ~/.git_template
[push]
  default = current
[color]
  ui = auto
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  aa = add --all
  ap = add --patch
[fetch]
  prune = true
[rebase]
  autosquash = true
[user]
	name = Cory Gwin
	email = gwincr11@yahoo.com
[core]
	excludesfile = /Users/corygwin/.gitignore
	editor = vim
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	required = true
	clean = git-lfs clean -- %f
