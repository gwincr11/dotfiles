[push]
  default = simple
[color]
  ui = auto
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  aa = add --all
  ap = add --patch
	sla = log --oneline --decorate --graph --all
	glog = log -E -i --grep
	unstage = reset
	uncommit = reset --soft HEAD^
[fetch]
  prune = true
[rebase]
  autosquash = true
[pull]
  rebase = false
[core]
	editor = vim
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	required = true
	clean = git-lfs clean -- %f
[user]
	name = Cory Gwin
[commit]
  template = ~/.committemplate
